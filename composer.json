{
  "name": "berlioz/html-selector",
  "description": "Berlioz HTML Selector",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "homepage": "https://getberlioz.com",
  "authors": [
    {
      "name": "Ronan Giron",
      "email": "ronan.giron@berlioz-framework.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Berlioz\\HtmlSelector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Berlioz\\HtmlSelector\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1 || ^8.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "berlioz/http-message": "^1.0",
    "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0"
  }
}
